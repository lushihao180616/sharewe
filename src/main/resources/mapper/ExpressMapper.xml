<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lushihao.sharewe.dao.ExpressMapper">
    <insert id="createExpress">
		INSERT INTO
			express
			(addressId,buildingId,statusId,reward,sendUserOpenId,deadTime,sendTime)
		VALUES
			(#{addressId},#{buildingId},1,#{reward},#{sendUserOpenId},#{deadTime},#{sendTime})
	</insert>
</mapper>